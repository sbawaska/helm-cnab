{
    "name": "helm",
    "schemaVersion": "v1.0.0-WD",
    "version": "0.0.0",
    "description": "Makes a helm chart available as a cnab bundle",
    "invocationImages": {
        "cnab": {
            "builder": "docker",
            "configuration": {
                "registry": "sbawaska"
            },
            "name": "cnab"
        }
    },
    "keywords": [
        "helm-cnab"
    ],
    "credentials": {
        "kubeconfig": {
            "path": "/root/.kube/config"
        }
    }
}
